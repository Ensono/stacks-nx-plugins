{
    "$schema": "http://json-schema.org/schema",
    "name": "next",
    "version": "0.0.1",
    "generators": {
        "init": {
            "factory": "./dist/generators/init/generator",
            "schema": "./dist/generators/init/schema.json",
            "description": "next init generator"
        },
        "next-auth": {
            "factory": "./dist/generators/next-auth/generator",
            "schema": "./dist/generators/next-auth/schema.json",
            "description": "Add NextAuth to a project"
        },
        "react-query": {
            "factory": "./dist/generators/react-query/generator",
            "schema": "./dist/generators/react-query/schema.json",
            "description": "react-query generator"
        },
        "storybook": {
            "factory": "./dist/generators/storybook/generator",
            "schema": "./dist/generators/storybook/schema.json",
            "description": "storybook generator"
        }
    }
}

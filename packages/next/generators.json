{
    "$schema": "http://json-schema.org/schema",
    "name": "next",
    "version": "0.0.1",
    "generators": {
        "init": {
            "factory": "./src/generators/init/generator",
            "schema": "./src/generators/init/schema.json",
            "description": "next init generator"
        },
        "init-deployment": {
            "factory": "./src/generators/init-deployment/generator",
            "schema": "./src/generators/init-deployment/schema.json",
            "description": "next init deployment generator"
        },
        "next-auth": {
            "factory": "./src/generators/next-auth/generator",
            "schema": "./src/generators/next-auth/schema.json",
            "description": "Add NextAuth to a project"
        },
        "next-auth-redis": {
            "factory": "./src/generators/next-auth-redis/generator",
            "schema": "./src/generators/next-auth-redis/schema.json",
            "description": "Add Redis for session management to NextAuth app"
        },
        "next-auth-redis-deployment": {
            "factory": "./src/generators/next-auth-redis-deployment/generator",
            "schema": "./src/generators/next-auth-redis-deployment/schema.json",
            "description": "Add Redis infra to NextAuth app"
        }
    }
}

{
    "name": "pact",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/pact/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/packages/pact",
                "main": "packages/pact/src/index.ts",
                "tsConfig": "packages/pact/tsconfig.lib.json",
                "assets": ["packages/pact/*.md"]
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint"
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "packages/pact/jest.config.ts"
            }
        }
    }
}

{
    "$schema": "http://json-schema.org/schema",
    "name": "rest-client",
    "version": "0.0.1",
    "generators": {
        "http-client": {
            "factory": "./src/generators/http-client/generator",
            "schema": "./src/generators/http-client/schema.json",
            "description": "http-client generator"
        },
        "client-endpoint": {
            "factory": "./src/generators/client-endpoint/generator",
            "schema": "./src/generators/client-endpoint/schema.json",
            "description": "client-endpoint generator"
        },
        "bump-version": {
            "factory": "./src/generators/bump-version/generator",
            "schema": "./src/generators/bump-version/schema.json",
            "description": "bump-version generator"
        },
        "openapi-client": {
            "factory": "./src/generators/openapi-client/generator",
            "schema": "./src/generators/openapi-client/schema.json",
            "description": "Generates rest client based on an openapi contract"
        }
    }
}
